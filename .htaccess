
php_flag display_startup_errors on
php_flag display_errors on
php_flag html_errors on
RewriteEngine on
RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d


#RewriteCond %{HTTP_HOST} ^(.+).serials.ru$
#RewriteRule (.*) http://.ru/$1?region=%1 [L,R=301,QSA]

RewriteRule ^/$ /index.php?index=$1 [L]
RewriteRule ^wiki/Файл:(.*)$ /index.php?photo=$1 [L]
#RewriteRule ^wiki/Categoría:(.*)$ /index.php?word=$1 [L]
RewriteRule ^wiki/(.*)/$ /index.php?word=$1 [L]
RewriteRule ^wiki/(.*)$ /index.php?word=$1 [L]
RewriteRule ^list0.php$ /index.php?list0=list0 [L]
RewriteRule ^list.php?id=(.*)$ /index.php?list=$1 [L]
#RewriteRule ^category$ category.php?word=$1 [L]
#RewriteRule ^category/$ category.php?word=$1 [L]
#RewriteRule ^w/index.php(.*)$ /404.php?word=$1 [R]


